        "import random\n",
        "player_score = 0\n",
        "computer_score = 0\n",
        "valid_entries = [0,1]\n",
        "while True:\n",
        "  predict = random.randint(0,1)\n",
        "  \n",
        "\n",
        "  \n",
        "  player_input = input(\"Please enter heads or tails, heads is 0, tails is 1: \")\n",
        "  player_input = int(player_input)\n",
        "  while player_input not in valid_entries:\n",
        "    print(\"Invalid Input!\")\n",
        "    player_input = input(\"Please enter either 0 or 1: \")\n",
        "    player_input = int(player_input)\n",
        "  print('you entered:, ', player_input)\n",
        "  print('computer predicted', predict)\n",
        "  if predict == player_input:\n",
        "    computer_score = computer_score + 1\n",
        "  else: \n",
        "    player_score = player_score + 1\n",
        "  print(\"computer score \", computer_score, \"player score \", player_score)  \n",
        "  if computer_score == 10:\n",
        "    print(\"Bad Luck, Computer Wins!\")\n",
        "    break\n",
        "  elif player_score == 10:\n",
        "    print(\"Congrats, You Won!\")\n",
        "    break"
